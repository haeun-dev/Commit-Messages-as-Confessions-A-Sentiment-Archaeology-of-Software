{
  "name": "codemood",
  "displayName": "CodeMood",
  "description": "Analyze code mood from your git repository",
  "version": "0.1.0",
  "publisher": "codemood",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:codemood.analyze"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codemood.analyze",
        "title": "Analyze Code Mood",
        "category": "CodeMood"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "codemood.analyze",
          "when": "resourceScheme == file",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "codemoodView",
          "name": "Code Mood"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codemood",
          "title": "Code Mood",
          "icon": "$(heart)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
